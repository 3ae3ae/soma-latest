{
  "manifest_version": 3,
  "name": "소마 최신순 멘토링",
  "version": "1.0.0.0",
  "icons": {
    "128": "128.png"
  },
  "description": "멘토링/특강을 최신순으로 표시합니다.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "https://swmaestro.org/*",
    "https://www.swmaestro.org/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.swmaestro.org/sw/mypage/userAnswer/history.do*", "https://swmaestro.org/sw/mypage/userAnswer/history.do*"],
      "js": ["/src/index.js"]
    },
    {
      "matches":["https://*.swmaestro.org/sw/mypage/mentoLec/list.do?menuNo=200046*", "https://swmaestro.org/sw/mypage/mentoLec/list.do?menuNo=200046*"],
      "js": ["/src/index.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "109.0"
    }
  }
}
